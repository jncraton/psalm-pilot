<!doctype html>
<html lang="en-US">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta name="description" content="Song pilot" />
    <link rel="stylesheet" type="text/css" href="style.css" />
    <title>Psalm Pilot</title>
  </head>
  <body>
    <header>
      <img src="Logo/Untitled-evt.png" alt="Psalm Pilot Logo">
      <h1>Psalm Pilot</h1>
    </header>
    <main>
      <table>
        <thead>
          <tr>
            <th>Hymn Title</th>
            <th>Hymn Author</th>
          </tr>
        </thead>
        <tbody>
          {% for hymn in hymns %}
          <tr>
            <td>{{ hymn.title }}</td>
            <td>{{ hymn.authors or ''}}</td>
          </tr>
          {% endfor %}
        </tbody>
      </table>
    </main>
  </body>
</html>
